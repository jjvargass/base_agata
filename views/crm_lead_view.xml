<odoo>
  <data>

    <record id="base_agata_crm_lead_view_form" model="ir.ui.view" >
      <field name="name">base_agata_crm_lead_view_form</field>
      <field name="model">crm.lead</field>
      <field name="inherit_id" ref="crm.crm_lead_view_form"/>
      <field name="arch" type="xml">
          <field name="tag_ids" position="after">
            <field name="linea_negocio_id" optional="show"/>
            <field name="tipo_solucion_id" optional="show"/>
            <field name="aliado_id" optional="show"/>
            <field name="estrategia_id" optional="show"/>
            <field name="presupuesto_id" optional="show"/>
          </field>
      </field>
    </record>

  </data>
</odoo>
